      addi            r4, r4, 4
      addi            r5, r5, -2
      cmpi            cr0, 0, r5, 0
      beq             cr0, .L.__done_TROT
.L.__test4_TROT:
      cmpi            cr0, 0, r5, 4			! are there 4 or more bytes left?
      blt             cr0, .L.__residue_TROT	        ! false, goto residue
      andi.           r6, r3, 0x7                       ! check if input is aligned at 8 bytes.
      beq             cr0, .L.__test8_TROT
.L.__dotest4_TROT:
