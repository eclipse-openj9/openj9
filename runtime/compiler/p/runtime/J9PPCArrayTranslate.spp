    vmrghb          vr2, vr0, vr1
    vmrglb          vr1, vr0, vr1
    stxvw4x         vs34, r0, r4                     ! store elements 0-7
    stxvw4x         vs33, r4, r6                    ! store elements 8-15

    addi            r3, r3, 16                      ! bump input ptr
    addi            r4, r4, 32                      ! bump output ptr
    bdnz            .L.__vectorLoop_TROT

