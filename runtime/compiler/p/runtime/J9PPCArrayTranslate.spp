        mfctr   r6                                !Get number of iterations left back.
        rlwimi  r5, r6, 4, 0, 31-4                ! r5 = r6 * 16 + r5, add remaining elements from vector loop and remainder back.
.L.__simple_TROT:
        cmpi    cr0, 0, r5, 0
        beq     cr0, .L.__done_TROT               ! if 0 elements left, we are done
