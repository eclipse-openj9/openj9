		andi.		r7, r6, 0x80
                bne             cr0, .L.__done_TROT
		sth             r6, 0(r4)
		addi            r3, r3, 1
		addi            r4, r4, 2
		addi            r5, r5, -1
		cmpi            cr0, 0, r5, 0
                bne             cr0, .L.__residue_TROT

.L.__done_TROT:
        laddr           r6, -ALen(J9SP)
        subf            r3, r6, r3                      ! r3 = output_ptr - output_base_ptr = num_elements_processed
        blr

