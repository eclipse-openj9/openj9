      addi            r3, r3, 1
      addi            r4, r4, 2
      addi            r5, r5, -1
      cmpi            cr0, 0, r5, 0
      beq             cr0, .L.__done_TROT
.L.__test2_TROT:					! aligned at 2 bytes.
      cmpi            cr0, 0, r5, 2			! are there 2 or more bytes left?
