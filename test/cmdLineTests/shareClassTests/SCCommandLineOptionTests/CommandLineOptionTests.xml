	<variable name="JAVA_DIR" value="$JDK_HOME$/bin"/>
	<variable name="JAVAC_DIR" value="$JDK_HOME$/bin"/>
	<variable name="JAVA_EXE" value="$JDK_HOME$/bin/java$EXECUTABLE_SUFFIX$"/>
